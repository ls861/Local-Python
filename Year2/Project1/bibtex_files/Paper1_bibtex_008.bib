}

@article{shivaei_mosdef_2015,
	title = {The {MOSDEF} {Survey}: {Dissecting} the {Star} {Formation} {Rate} versus {Stellar} {Mass} {Relation} {Using} {Hα} and {Hβ} {Emission} {Lines} at z ∼ 2},
	volume = {815},
	issn = {0004-637X},
	shorttitle = {The {MOSDEF} {Survey}},
	url = {https://ui.adsabs.harvard.edu/abs/2015ApJ...815...98S},
	doi = {10.1088/0004-637X/815/2/98},
	abstract = {We present results on the star formation rate (SFR) versus stellar mass (M*) relation (i.e., the “main sequence”) among star-forming galaxies at 1.37 ≤ z ≤ 2.61 using the MOSFIRE Deep Evolution Field (MOSDEF) survey. Based on a sample of 261 galaxies with Hα and Hβ spectroscopy, we have estimated robust dust-corrected instantaneous SFRs over a large range in M* (∼109.5-1011.5 M⊙). We find a correlation between log(SFR(Hα)) and log(M*) with a slope of 0.65 ± 0.08 (0.58 ± 0.10) at 1.4 {\textless} z {\textless} 2.6 (2.1 {\textless} z {\textless} 2.6). We find that different assumptions for the dust correction, such as using the color excess of the stellar continuum to correct the nebular lines, sample selection biases against red star-forming galaxies, and not accounting for Balmer absorption, can yield steeper slopes of the log(SFR)-log(M*) relation. Our sample is immune from these biases as it is rest-frame optically selected, Hα and Hβ are corrected for Balmer absorption, and the Hα luminosity is dust corrected using the nebular color excess computed from the Balmer decrement. The scatter of the log(SFR(Hα))-log(M*) relation, after accounting for the measurement uncertainties, is 0.31 dex at 2.1 {\textless} z {\textless} 2.6, which is 0.05 dex larger than the scatter in log(SFR(UV))-log(M*). Based on comparisons to a simulated SFR-M* relation with some intrinsic scatter, we argue that in the absence of direct measurements of galaxy-to-galaxy variations in the attenuation/extinction curves and the initial mass function, one cannot use the difference in the scatter of the SFR(Hα)- and SFR(UV)-M* relations to constrain the stochasticity of star formation in high-redshift galaxies.},
	urldate = {2021-11-09},
	journal = {ApJ},
	author = {Shivaei, Irene and Reddy, Naveen A. and Shapley, Alice E. and Kriek, Mariska and Siana, Brian and Mobasher, Bahram and Coil, Alison L. and Freeman, William R. and Sanders, Ryan and Price, Sedona H. and de Groot, Laura and Azadi, Mojegan},
	month = dec,
	year = {2015},
	note = {ADS Bibcode: 2015ApJ...815...98S},
	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: evolution, galaxies: star formation, galaxies: high-redshift, galaxies: formation},
	pages = {98},
	file = {Accepted Version:/Users/lester/Zotero/storage/NUGGN9DG/Shivaei et al. - 2015 - The MOSDEF Survey Dissecting the Star Formation R.pdf:application/pdf},
}

@article{whitaker_galaxy_2015,
	title = {Galaxy {Structure} as a {Driver} of the {Star} {Formation} {Sequence} {Slope} and {Scatter}},
	volume = {811},
	issn = {0004-637X},
	url = {https://ui.adsabs.harvard.edu/abs/2015ApJ...811L..12W},
	doi = {10.1088/2041-8205/811/1/L12},
	abstract = {It is well established that (1) star-forming galaxies follow a relation between their star formation rate (SFR) and stellar mass (\{M\}{\textbackslash}star ), the “star formation sequence,” and (2) the SFRs of galaxies correlate with their structure, where star-forming galaxies are less concentrated than quiescent galaxies at fixed mass. Here, we consider whether the scatter and slope of the star formation sequence is correlated with systematic variations in the Sérsic indices, n, of galaxies across the SFR-\{M\}{\textbackslash}star plane. We use a mass-complete sample of 23,848 galaxies at 0.5 {\textless} z {\textless} 2.5 selected from the 3D-HST photometric catalogs. Galaxy light profiles parameterized by n are based on Hubble Space Telescope Cosmic Assembly Near-IR Deep Extragalactic Legacy Survey near-infrared imaging. We use a single SFR indicator empirically calibrated from stacks of Spitzer/MIPS 24 μm imaging, adding the unobscured and obscured star formation. We find that the scatter of the star formation sequence is related in part to galaxy structure; the scatter due to variations in n at fixed mass for star-forming galaxies ranges from 0.14 ± 0.02 dex at z ∼ 2 to 0.30 ± 0.04 dex at z {\textless} 1. While the slope of the \{log\} \{SFR\}-\{log\} \{M\}{\textbackslash}star relation is of order unity for disk-like galaxies, galaxies with n {\textgreater} 2 (implying more dominant bulges) have significantly lower \{SFR\}/\{M\}{\textbackslash}star than the main ridgeline of the star formation sequence. These results suggest that bulges in massive z ∼ 2 galaxies are actively building up, where the stars in the central concentration are relatively young. At z {\textless} 1, the presence of older bulges within star-forming galaxies lowers global \{SFR\}/\{M\}{\textbackslash}star , decreasing the slope and contributing significantly to the scatter of the star formation sequence.},
