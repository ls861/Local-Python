@article{steidel_lyman-break_1999,
	title = {Lyman-{Break} {Galaxies} at z{\textgreater}{\textasciitilde}4 and the {Evolution} of the {Ultraviolet} {Luminosity} {Density} at {High} {Redshift}},
	volume = {519},
	issn = {0004-637X},
	url = {https://ui.adsabs.harvard.edu/abs/1999ApJ...519....1S},
	doi = {10.1086/307363},
	abstract = {We present initial results of a survey for star-forming galaxies in the redshift range 3.8{\textless}{\textasciitilde}z{\textless}{\textasciitilde}4.5. This sample consists of a photometric catalog of 244 galaxies culled from a total solid angle of 0.23 deg2 to an apparent magnitude of IAB=25.0. Spectroscopic redshifts in the range 3.61{\textless}=z{\textless}=4.81 have been obtained for 48 of these galaxies; their median redshift is =4.13. Selecting these galaxies in a manner entirely analogous to our large survey for Lyman-break galaxies at smaller redshift (2.7{\textless}{\textasciitilde}z{\textless}{\textasciitilde}3.4) allows a relatively clean differential comparison between the populations and integrated luminosity density at these two cosmic epochs. Over the same range of UV luminosity, the spectroscopic properties of the galaxy samples at z{\textasciitilde}4 and z{\textasciitilde}3 are indistinguishable, as are the luminosity function shapes and the total integrated UV luminosity densities [ρUV(z=3)/ρUV(z=4)=1.1+/-0.3]. We see no evidence at these bright magnitudes for the steep decline in the star formation density inferred from fainter photometric Lyman-break galaxies in the Hubble deep field (HDF). The HDF provides the only existing data on Lyman-break galaxy number densities at fainter magnitudes. We have reanalyzed the z{\textasciitilde}3 and z{\textasciitilde}4 Lyman-break galaxies in the HDF using our improved knowledge of the spectral energy distributions of these galaxies, and we find, like previous authors, that faint Lyman-break galaxies appear to be rarer at z{\textasciitilde}4 than z{\textasciitilde}3. This might signal a large change in the faint-end slope of the Lyman-break galaxy luminosity function between redshifts z{\textasciitilde}3 and z{\textasciitilde}4, or, more likely, be due to significant variance in the number counts within the small volumes probed by the HDF at high redshifts ({\textasciitilde}160 times smaller than the ground-based surveys discussed here). If the true luminosity density at z{\textasciitilde}4 is somewhat higher than implied by the HDF, as our ground-based sample suggests, then the emissivity of star formation as a function of redshift would appear essentially constant for all z{\textgreater}1 once internally consistent corrections for dust are made. This suggests that there may be no obvious peak in star formation activity and that the onset of substantial star formation in galaxies might occur at z{\textgreater}{\textasciitilde}4.5. Based on data obtained at the Palomar Observatory, the Cerro-Tololo Inter-American Observatory, the William Herschel Telescope, and the W. M. Keck Observatory. The W. M. Keck Observatory is operated as a scientific partnership among the California Institute of Technology, the University of California, and NASA, and was made possible by the generous financial support of the W. M. Keck Foundation.},
	urldate = {2021-11-23},
	journal = {ApJ},
	author = {Steidel, Charles C. and Adelberger, Kurt L. and Giavalisco, Mauro and Dickinson, Mark and Pettini, Max},
	month = jul,
	year = {1999},
	note = {ADS Bibcode: 1999ApJ...519....1S},
	keywords = {Astrophysics, Galaxies: Evolution, Galaxies: Distances and Redshifts, Catalogs, CATALOGS, GALAXIES: DISTANCES AND REDSHIFTS, GALAXIES: EVOLUTION, Galaxies: Statistics, GALAXIES: STATISTICS, Galaxies: Stellar Content, GALAXIES: STELLAR CONTENT},
	pages = {1--17},
	file = {Full Text PDF:/Users/lester/Zotero/storage/GKZXX28P/Steidel et al. - 1999 - Lyman-Break Galaxies at z~4 and the Evolution of .pdf:application/pdf},
}

@article{madau_high-redshift_1996,
	title = {High-redshift galaxies in the {Hubble} {Deep} {Field}: colour selection and star formation history to z{\textasciitilde}4},
	volume = {283},
	issn = {0035-8711},
	shorttitle = {High-redshift galaxies in the {Hubble} {Deep} {Field}},
	url = {https://ui.adsabs.harvard.edu/abs/1996MNRAS.283.1388M},
	doi = {10.1093/mnras/283.4.1388},
	abstract = {The Lyman decrement associated with the cumulative effect of HI in QSO absorption systems along the line of sight provides a distinctive feature for identifying galaxies at z{\textgreater}{\textasciitilde}2.5. Colour criteria, which are sensitive to the presence of a Lyman continuum break superposed on an otherwise flat UV spectrum, have been shown, through Keck spectroscopy, to successfully identify a substantial population of star-forming galaxies at 3{\textless}{\textasciitilde}z{\textless}{\textasciitilde}3.5. Such objects have proven to be surprisingly elusive in field galaxy redshift surveys; quantification of their surface densities and morphologies is crucial for determining how and when galaxies formed. The Hubble Deep Field (HDF) observations offer the opportunity to exploit the ubiquitous effect of intergalactic absorption and obtain useful statistical constraints on the redshift distribution of galaxies to considerably fainter limits than the current spectroscopic limits. We model the HI cosmic opacity as a function of redshift, including scattering in resonant lines of the Lyman series and Lyman continuum absorption, and use stellar population synthesis models with a wide variety of ages, metallicities, dust contents and redshifts to derive colour selection criteria that provide a robust separation between high-redshift and low-redshift galaxies. From the HDF images we construct a sample of star-forming galaxies at 2{\textless}{\textasciitilde}z{\textless}{\textasciitilde}4.5. While none of the {\textasciitilde}60 objects in the HDF having known Keck/Low-Resolution Imaging Spectrograph (LRIS) spectroscopic redshifts in the range 0{\textless}{\textasciitilde}z{\textless}{\textasciitilde}1.4 is found to contaminate our high-redshift sample, our colour criteria are able to efficiently select the 2.6{\textless}{\textasciitilde}z{\textless}{\textasciitilde}3.2 galaxies identified by Steidel et al. The ultraviolet (and blue) dropout technique opens up the possibility of investigating cosmic star and element formation in the early Universe. We set a lower limit to the ejection rate of heavy elements per unit comoving volume from Type II supernovae at =2.75 of {\textasciitilde}=3.6x10{\textasciicircum}-4 M\_solar yr{\textasciicircum}-1 Mpc{\textasciicircum}-3 (for q\_0=0.5 and H\_0=50 km s{\textasciicircum}-1 Mpc{\textasciicircum}-1), which is 3 times higher than the local value but still 4 times lower than the rate observed at z{\textasciitilde}=1. At =4, our lower limit to the cosmic metal ejection rate is {\textasciitilde}=3 times lower than the =2.75 value. We discuss the implications of these results on models of galaxy formation, and on the chemical enrichment and ionization history of the intergalactic medium.},
	urldate = {2021-11-23},
	journal = {MNRAS},
	author = {Madau, Piero and Ferguson, Henry C. and Dickinson, Mark E. and Giavalisco, Mauro and Steidel, Charles C. and Fruchter, Andrew},
	month = dec,
	year = {1996},
	note = {ADS Bibcode: 1996MNRAS.283.1388M},
	keywords = {Astrophysics, STARS: FORMATION, ULTRAVIOLET: GALAXIES, GALAXIES: EVOLUTION, COSMOLOGY: OBSERVATIONS, INTERGALACTIC MEDIUM, QUASARS: ABSORPTION LINES},
	pages = {1388--1404},
	file = {Full Text PDF:/Users/lester/Zotero/storage/CB7FH5JH/Madau et al. - 1996 - High-redshift galaxies in the Hubble Deep Field c.pdf:application/pdf},
}

