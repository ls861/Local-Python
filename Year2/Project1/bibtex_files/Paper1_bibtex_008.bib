@article{tasca_evolving_2015,
	title = {The evolving star formation rate: {M}{\textless}{SUB}{\textgreater}⋆{\textless}/{SUB}{\textgreater} relation and {sSFR} since z ≃ 5 from the {VUDS} spectroscopic survey},
	volume = {581},
	issn = {0004-6361},
	shorttitle = {The evolving star formation rate},
	url = {https://ui.adsabs.harvard.edu/abs/2015A&A...581A..54T/abstract},
	doi = {10.1051/0004-6361/201425379},
	abstract = {We study the evolution of the star formation rate (SFR) - stellar mass (M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater}) relation and specific star formation rate (sSFR) of star-forming galaxies (SFGs) since a redshift z ≃ 5.5 using 2435 (4531) galaxies with highly reliable spectroscopic redshifts in the VIMOS Ultra-Deep Survey (VUDS). It is the first time that these relations can be followed over such a large redshift range from a single homogeneously selected sample of galaxies with spectroscopic redshifts. The log (SFR) - log (M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater}) relation for SFGs remains roughly linear all the way up to z = 5, but the SFR steadily increases at fixed mass with increasing redshift. We find that for stellar masses M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater} ≥ 3.2 × 10{\textless}SUP{\textgreater}9{\textless}/SUP{\textgreater}M{\textless}SUB{\textgreater}⊙{\textless}/SUB{\textgreater} the SFR increases by a factor of {\textasciitilde}13 between z = 0.4 and z = 2.3. Weextend this relation up to z = 5, finding an additional increase in SFR by a factor of 1.7 from z = 2.3 to z = 4.8 for masses M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater} ≥ 10{\textless}SUP{\textgreater}10{\textless}/SUP{\textgreater}M{\textless}SUB{\textgreater}⊙{\textless}/SUB{\textgreater}. We observe a turn-off in the SFR-M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater} relation at the highest mass end up to a redshift z {\textasciitilde} 3.5. We interpret this turn-off as the signature of a strong on-going quenching mechanism and rapid mass growth. The sSFR increases strongly up to z {\textasciitilde} 2, but it grows much less rapidly in 2 \&lt;z\&lt; 5. We find that the shape of the sSFR evolution is not well reproduced by cold gas accretion-driven models or the latest hydrodynamical models. Below z {\textasciitilde} 2 these models have a flatter evolution (1 + z){\textless}SUP{\textgreater}Φ{\textless}/SUP{\textgreater} with Φ = 2 - 2.25 compared to the data which evolves more rapidly with Φ = 2.8 ± 0.2. Above z {\textasciitilde} 2, the reverse is happening with the data evolving more slowly with Φ = 1.2 ± 0.1. The observed sSFR evolution over a large redshift range 0 \&lt;z\&lt; 5 and our finding of a non-linear main sequence at high mass both indicate that the evolution of SFR and M{\textless}SUB{\textgreater}⋆{\textless}/SUB{\textgreater} is not solely driven by gas accretion. The results presented in this paper emphasize the need to invoke a more complex mix of physical processes including major and minor merging to further understand the co-evolution of the SFR and stellar mass growth. {\textless}P /{\textgreater}Based on data obtained with the European Southern Observatory Very Large Telescope, Paranal, Chile, under Large Program 185.A-0791.},
	language = {en},
	urldate = {2021-11-09},
