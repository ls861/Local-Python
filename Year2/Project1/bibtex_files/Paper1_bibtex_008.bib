	pages = {117--128},
	file = {Full Text PDF:/Users/lester/Zotero/storage/6MG8MC3K/Koprowski et al. - 2014 - A reassessment of the redshift distribution and ph.pdf:application/pdf},
}

@article{speagle_highly_2014,
