	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: abundances, galaxies: general, galaxies: high-redshift, galaxies: ISM},
	pages = {1757--1774},
	file = {Full Text PDF:/Users/lester/Zotero/storage/KV9NKIU6/Gutkin et al. - 2016 - Modelling the nebular emission from primeval to pr.pdf:application/pdf},
}

