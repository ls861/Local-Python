	month = aug,
	year = {1999},
	note = {ADS Bibcode: 1999ApJ...521...64M},
	keywords = {Astrophysics, Ultraviolet: Galaxies, Galaxies: ISM, Infrared: Galaxies, GALAXIES: ISM, Galaxies: Photometry, GALAXIES: PHOTOMETRY, INFRARED: GALAXIES, ULTRAVIOLET: GALAXIES},
	pages = {64--80},
	file = {Full Text PDF:/Users/lester/Zotero/storage/SKJ2LCDQ/Meurer et al. - 1999 - Dust Absorption and the Ultraviolet Luminosity Den.pdf:application/pdf},
