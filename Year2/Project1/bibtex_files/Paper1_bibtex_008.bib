@article{reddy_characteristic_2012,
	title = {The {Characteristic} {Star} {Formation} {Histories} of {Galaxies} at {Redshifts} z {\textasciitilde} 2-7},
	volume = {754},
	issn = {0004-637X},
	url = {https://ui.adsabs.harvard.edu/abs/2012ApJ...754...25R},
	doi = {10.1088/0004-637X/754/1/25},
	abstract = {A large sample of spectroscopically confirmed star-forming galaxies at redshifts 1.4 {\textless}= z spec {\textless}= 3.7, with complementary imaging in the near- and mid-IR from the ground and from the Hubble Space Telescope and Spitzer Space Telescope, is used to infer the average star formation histories (SFHs) of typical galaxies from z {\textasciitilde} 2 to 7. For a subset of 302 galaxies at 1.5 {\textless}= z spec {\textless} 2.6, we perform a detailed comparison of star formation rates (SFRs) determined from spectral energy distribution (SED) modeling (SFRs[SED]) and those calculated from deep Keck UV and Spitzer/MIPS 24 μm imaging (SFRs[IR+UV]). Exponentially declining SFHs yield SFRs[SED] that are 5-10 times lower on average than SFRs[IR+UV], indicating that declining SFHs may not be accurate for typical galaxies at z {\textgreater}{\textasciitilde} 2. The SFRs of z {\textasciitilde} 2-3 galaxies are directly proportional to their stellar masses (M *), with unity slope—a result that is confirmed with Spitzer/IRAC stacks of 1179 UV-faint (\{{\textbackslash}cal R\}{\textgreater}25.5) galaxies—for M * {\textgreater}{\textasciitilde} 5 × 108 M ⊙ and SFRs {\textgreater}{\textasciitilde} 2 M ⊙ yr-1. We interpret this result in the context of several systematic biases that can affect determinations of the SFR-M * relation. The average specific SFRs at z {\textasciitilde} 2-3 are remarkably similar within a factor of two to those measured at z {\textgreater}{\textasciitilde} 4, implying that the average SFH is one where SFRs increase with time. A consequence of these rising SFHs is that (1) a substantial fraction of UV-bright z {\textasciitilde} 2-3 galaxies had faint sub-L* progenitors at z {\textgreater}{\textasciitilde} 4; and (2) gas masses must increase with time from z = 2 to 7, over which time the net cold gas accretion rate—as inferred from the specific SFR and the Kennicutt-Schmidt relation—is {\textasciitilde}2-3 times larger than the SFR. However, if we evolve to higher redshift the SFHs and masses of the halos that are expected to host L* galaxies at z {\textasciitilde} 2, then we find that {\textless}{\textasciitilde} 10\% of the baryons accreted onto typical halos at z {\textgreater}{\textasciitilde} 4 actually contribute to star formation at those epochs. These results highlight the relative inefficiency of star formation even at early cosmic times when galaxies were first assembling. Based, in part, on data obtained at the W. M. Keck Observatory, which is operated as a scientific partnership among the California Institute of Technology, the University of California, and NASA, and made possible by the generous financial support of the W. M. Keck Foundation.},
	urldate = {2021-11-09},
	journal = {ApJ},
	author = {Reddy, Naveen A. and Pettini, Max and Steidel, Charles C. and Shapley, Alice E. and Erb, Dawn K. and Law, David R.},
	month = jul,
