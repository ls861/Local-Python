	note = {ADS Bibcode: 2010AJ....139.2097P},
	keywords = {techniques: photometric, Astrophysics - Cosmology and Nongalactic Astrophysics, galaxies: fundamental parameters, galaxies: bulges, galaxies: structure, techniques: image processing},
	pages = {2097--2129},
	file = {Submitted Version:/Users/lester/Zotero/storage/8JSYNAPB/Peng et al. - 2010 - Detailed Decomposition of Galaxy Images. II. Beyon.pdf:application/pdf},
}

@article{feroz_multinest_2009,
