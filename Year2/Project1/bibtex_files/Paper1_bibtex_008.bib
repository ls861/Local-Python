	note = {ADS Bibcode: 2009ApJ...705..617D},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, galaxies: evolution, galaxies: high-redshift, galaxies: formation},
	pages = {617--623},
	file = {Submitted Version:/Users/lester/Zotero/storage/KYEA8R2C/Damen et al. - 2009 - Star Formation in the Chandra Deep Field South Ob.pdf:application/pdf},
