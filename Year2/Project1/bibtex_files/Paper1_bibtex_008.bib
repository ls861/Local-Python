	note = {ADS Bibcode: 2016MNRAS.462.1415C},
	keywords = {Astrophysics - Astrophysics of Galaxies, galaxies: evolution, methods: data analysis, galaxies: stellar content, methods: statistical, dust, extinction, H II regions},
	pages = {1415--1443},
	file = {Full Text PDF:/Users/lester/Zotero/storage/CQR4M8RI/Chevallard and Charlot - 2016 - Modelling and interpreting spectral energy distrib.pdf:application/pdf},
}

